{{#*inline "resItem"}}
<tr>
{{#if include}}
<td class="name">({{name}})</td>
{{else}}
<td class="name">{{name}}</td>
{{/if}}
<td class="desc">{{{desc}}}</td>
</tr>
{{/inline}}

<h3>Object</h3>

<table>
{{#each params}}
{{#with (lookup ../info @this)}}
{{#unless root}}
{{> resItem name=../this}}
{{/unless}}
{{/with}}
{{/each}}
</table>

<h3>Root</h3>
<table>
{{#each params}}
{{#with (lookup ../info @this)}}
{{#if root}}
{{> resItem name=../this}}
{{/if}}
{{/with}}
{{/each}}
</table>
