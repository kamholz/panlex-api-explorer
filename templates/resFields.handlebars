{{#*inline "resTable"}}
<table class="table">
<tr><th class="name">field</th><th class="type">type</th><th class="desc">description</th></tr>
{{#eachSorted fields}}
  <tr>
  <td class="name">{{name}}{{> restrictionInfoIcon restriction=attr.restriction}}</td>
  <td class="type">{{attr.type}}{{> typeInfoIcon type=(lookup ../types attr.type)}}</td>
  <td class="desc">{{{attr.desc}}}</td>
  </tr>
{{/eachSorted}}
</table>
{{/inline}}

<h2>Response fields</h2>

{{#if fields}}
  <h3>Result Object</h3>
  {{> resTable fields=fields}}
{{/if}}

<h3>Root</h3>
{{> resTable fields=fieldsRoot}}
