{{#eachSorted types}}
  <div class="typeDescription">
  <h3>Type <code>{{name}}</code></h3>
  <p>{{{attr.desc}}}</p>
  {{#if attr.fields}}
    <table class="table">
      <tr><th class="name">field</th><th class="type">type</th><th class="desc">description</th></tr>
      {{#eachSorted attr.fields}}
        <tr><td class="name">{{name}}</td><td class="type">{{attr.type}}</td><td class="desc">{{{attr.desc}}}</td></tr>
      {{/eachSorted}}
    </table>
  {{/if}}
  {{#if attr.example}}
    <p><em>Example:</em> {{{attr.example}}}</p>
  {{/if}}
  </div>
{{/eachSorted}}
